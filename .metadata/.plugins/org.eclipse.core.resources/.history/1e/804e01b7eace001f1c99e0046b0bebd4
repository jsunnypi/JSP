package com.test.odd;

import java.util.Scanner;

public class SumOdd {

	public static void main(String[] args) {
		char ch1 = 'A';
		String name = "홍길동";
		
		System.out.printf("%c\n", ch1);
		System.out.printf("%s\n", "AB");
		System.out.printf("%s\n", "AB CCCC");
		System.out.printf("%s씨 %s\n%s\n", name, "재미있는 자바언어", "열심히해");
	}

}
