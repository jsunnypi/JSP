<script>

function sumOdd(array){
	
	int sum = 0;
	
	for(int i=0; i < array.length; i++){
		
		if(array[i]%2 ==1){
			sum += array[i]	
		}
		
	}
	
	const result = sumOdd([1,2,3,4,5]);
	console.log(result)
	
	
	
}



</script>
